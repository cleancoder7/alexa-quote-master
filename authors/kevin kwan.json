            {
                "name": "Asian",
                "url": "/topics/asian"
            },
            {
                "name": "Everyone",
                "url": "/topics/everyone"
            }
        ]
    },
    {
        "url": "/quotes/kevin_kwan_726196",
        "quote": "All Americans knew was 'The Joy Luck Club' and children of dry cleaners trying to assimilate. The Asia that I was seeing was a world of people who are incredibly sophisticated, and I wanted to represent that side.",
        "author": {
            "name": "Kevin Kwan",
            "url": "/quotes/kevin_kwan_726196"
        },
        "topics": [
            {
                "name": "Luck",
                "url": "/topics/luck"
