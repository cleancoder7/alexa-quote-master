            {
                "name": "Big",
                "url": "/topics/big"
            },
            {
                "name": "Players",
                "url": "/topics/players"
            }
        ]
    },
    {
        "url": "/quotes/tony_mccoy_628566",
        "quote": "I don't know which is stronger: the thrill of winning or the fear of failure. I don't know which one affects me most. I don't know where the middle is.",
        "author": {
            "name": "Tony McCoy",
            "url": "/quotes/tony_mccoy_628566"
        },
        "topics": [
            {
                "name": "Winning",
                "url": "/topics/winning"
