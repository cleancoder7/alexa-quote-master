            {
                "name": "Ride",
                "url": "/topics/ride"
            },
            {
                "name": "Hot",
                "url": "/topics/hot"
            }
        ]
    },
    {
        "url": "/quotes/tony_mccoy_628570",
        "quote": "I probably don't look healthy, but I have never got to the stage where I thought I was going to pass out.",
        "author": {
            "name": "Tony McCoy",
            "url": "/quotes/tony_mccoy_628570"
        },
        "topics": [
            {
                "name": "Thought",
