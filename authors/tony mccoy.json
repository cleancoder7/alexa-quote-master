            {
                "name": "Through",
                "url": "/topics/through"
            },
            {
                "name": "Like",
                "url": "/topics/like"
            }
        ]
    },
    {
        "url": "/quotes/tony_mccoy_628572",
        "quote": "During every race, an ambulance trails the riders around the course. You know that sometimes you are going to end up in the back of that ambulance.",
        "author": {
            "name": "Tony McCoy",
            "url": "/quotes/tony_mccoy_628572"
        },
        "topics": [
            {
                "name": "Race",
                "url": "/topics/race"
