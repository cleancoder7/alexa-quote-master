[
    {
        "url": "/quotes/stacy_schiff_528923",
        "quote": "I think with every book you realize you are partway through and there is something really elementary that you should have researched.",
        "author": {
            "name": "Stacy Schiff",
            "url": "/quotes/stacy_schiff_528923"
        },
        "topics": [
            {
                "name": "Think",
