            {
                "name": "Bad",
                "url": "/topics/bad"
            },
            {
                "name": "Mice",
                "url": "/topics/mice"
            }
        ]
    },
    {
        "url": "/quotes/mary_ellen_mark_380377",
        "quote": "Reality is always extraordinary.",
        "author": {
            "name": "Mary Ellen Mark",
            "url": "/quotes/mary_ellen_mark_380377"
        },
        "topics": [
            {
                "name": "Always",
