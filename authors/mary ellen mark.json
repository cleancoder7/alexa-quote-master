            {
                "name": "Extraordinary",
                "url": "/topics/extraordinary"
            }
        ]
    },
    {
        "url": "/quotes/mary_ellen_mark_499043",
        "quote": "I'm just interested in what makes a photograph.",
        "author": {
            "name": "Mary Ellen Mark",
            "url": "/quotes/mary_ellen_mark_499043"
        },
        "topics": [
            {
                "name": "Just",
                "url": "/topics/just"
