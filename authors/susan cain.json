    {
        "url": "/quotes/susan_cain_538549",
        "quote": "I use a lot of old-fashioned expressions.",
        "author": {
            "name": "Susan Cain",
            "url": "/quotes/susan_cain_538549"
        },
        "topics": [
            {
                "name": "Old-Fashioned",
