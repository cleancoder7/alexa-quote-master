            {
                "name": "Notes",
                "url": "/topics/notes"
            },
            {
                "name": "Rhythms",
                "url": "/topics/rhythms"
            }
        ]
    },
    {
        "url": "/quotes/jason_moran_774566",
        "quote": "In school, I did a lot of computer work. I'd take splices from Kurt Weill songs and loop them in bars, in beats of seven, trying all different kinds of things.",
        "author": {
            "name": "Jason Moran",
            "url": "/quotes/jason_moran_774566"
        },
        "topics": [
            {
                "name": "Work",
                "url": "/topics/work"
