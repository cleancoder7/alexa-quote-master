[
    {
        "url": "/quotes/gary_neville_422242",
        "quote": "With good coaching, proper motivation and the right club structure with organic growth, you can achieve an awful lot in football.",
        "author": {
            "name": "Gary Neville",
            "url": "/quotes/gary_neville_422242"
        },
        "topics": [
            {
                "name": "Achieve",
