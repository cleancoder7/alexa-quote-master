            {
                "name": "Fact",
                "url": "/topics/fact"
            },
            {
                "name": "No Sense",
                "url": "/topics/no_sense"
            }
        ]
    },
    {
        "url": "/quotes/bjarke_ingels_856069",
        "quote": "A kid in Minecraft can build a world and inhabit it through play. We have the possibility to build the world that we want to inhabit.",
        "author": {
            "name": "Bjarke Ingels",
            "url": "/quotes/bjarke_ingels_856069"
