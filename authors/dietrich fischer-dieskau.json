            {
                "name": "Line",
                "url": "/topics/line"
            },
            {
                "name": "Also",
                "url": "/topics/also"
            }
        ]
    },
    {
        "url": "/quotes/dietrich_fischerdieskau_206198",
        "quote": "Many, many composers have only found their way to a certain form, through familiarizing themselves with texts.",
        "author": {
            "name": "Dietrich Fischer-Dieskau",
            "url": "/quotes/dietrich_fischerdieskau_206198"
        },
        "topics": [
            {
                "name": "Through",
                "url": "/topics/through"
