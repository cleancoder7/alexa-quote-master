            {
                "name": "Other",
                "url": "/topics/other"
            },
            {
                "name": "Schubert",
                "url": "/topics/schubert"
            }
        ]
    },
    {
        "url": "/quotes/dietrich_fischerdieskau_206197",
        "quote": "The composition of a single melody is born out of a bit of text, perhaps the first line, but it can also be the entire strophe; it can even be the poem's overall form.",
        "author": {
            "name": "Dietrich Fischer-Dieskau",
            "url": "/quotes/dietrich_fischerdieskau_206197"
        },
        "topics": [
            {
                "name": "Born",
                "url": "/topics/born"
