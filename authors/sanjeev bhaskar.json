            {
                "name": "Quoting",
                "url": "/topics/quoting"
            },
            {
                "name": "Surreal",
                "url": "/topics/surreal"
            }
        ]
    },
    {
        "url": "/quotes/sanjeev_bhaskar_618239",
        "quote": "One of the benefits for me of starting late in this business is I realised that if acting was the only thing I could do, I would struggle, so I always wrote as well.",
        "author": {
            "name": "Sanjeev Bhaskar",
            "url": "/quotes/sanjeev_bhaskar_618239"
        },
        "topics": [
            {
                "name": "Always",
                "url": "/topics/always"
