            {
                "name": "Arthur",
                "url": "/topics/arthur"
            },
            {
                "name": "Trading",
                "url": "/topics/trading"
            }
        ]
    },
    {
        "url": "/quotes/sanjeev_bhaskar_618238",
        "quote": "When we created 'Goodness Gracious Me,' it was quoting 'Python' and Woody Allen lines that really bonded the writers, and the 'Spamalot' material is so utterly, wonderfully surreal that it hasn't dated.",
