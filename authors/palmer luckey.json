    {
        "url": "/quotes/palmer_luckey_786718",
        "quote": "VR is going to be defined by the content that is designed explicitly for virtual reality.",
        "author": {
            "name": "Palmer Luckey",
            "url": "/quotes/palmer_luckey_786718"
        },
        "topics": [
            {
                "name": "Going",
