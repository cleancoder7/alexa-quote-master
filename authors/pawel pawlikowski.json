    {
        "url": "/quotes/pawel_pawlikowski_774490",
        "quote": "Art is not journalism. In art, you don't make it to convey a message.",
        "author": {
            "name": "Pawel Pawlikowski",
            "url": "/quotes/pawel_pawlikowski_774490"
