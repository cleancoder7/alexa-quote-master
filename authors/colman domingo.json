            }
        ]
    },
    {
        "url": "/quotes/colman_domingo_687366",
        "quote": "My mother was the love of my life.",
        "author": {
            "name": "Colman Domingo",
            "url": "/quotes/colman_domingo_687366"
        },
        "topics": [
