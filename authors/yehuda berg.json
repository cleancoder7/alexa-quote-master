            }
        ]
    },
    {
        "url": "/quotes/yehuda_berg_536679",
        "quote": "I've learnt more physics from Kabbalah than I ever did in school.",
        "author": {
            "name": "Yehuda Berg",
            "url": "/quotes/yehuda_berg_536679"
        },
