            }
        ]
    },
    {
        "url": "/quotes/eric_davis_421310",
        "quote": "Everyone would like to play in their hometown, but right now I like Cincinnati, I like the way it's going. I'm happy.",
        "author": {
            "name": "Eric Davis",
            "url": "/quotes/eric_davis_421310"
        },
        "topics": [
            {
                "name": "Play",
                "url": "/topics/play"
