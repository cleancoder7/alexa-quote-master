            },
            {
                "name": "Sensation",
                "url": "/topics/sensation"
            }
        ]
    },
    {
        "url": "/quotes/samuel_alexander_300749",
        "quote": "An expectation is a future object, recognised as belonging to me.",
        "author": {
            "name": "Samuel Alexander",
            "url": "/quotes/samuel_alexander_300749"
        },
        "topics": [
            {
                "name": "Me",
