            {
                "name": "Big",
                "url": "/topics/big"
            },
            {
                "name": "Replied",
                "url": "/topics/replied"
            }
        ]
    },
    {
        "url": "/quotes/charles_m_schwab_373650",
        "quote": "I will not be in the position of having management dictated to by labor.",
        "author": {
            "name": "Charles M. Schwab",
            "url": "/quotes/charles_m_schwab_373650"
        },
        "topics": [
