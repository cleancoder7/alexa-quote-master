    },
    {
        "url": "/quotes/agnes_gund_884613",
        "quote": "I have always had an extreme sensitivity to inequality.",
        "author": {
            "name": "Agnes Gund",
            "url": "/quotes/agnes_gund_884613"
        },
        "topics": [
            {
                "name": "Inequality",
