            {
                "name": "Actress",
                "url": "/topics/actress"
            },
            {
                "name": "Movie",
                "url": "/topics/movie"
            }
        ]
    },
    {
        "url": "/quotes/lou_doillon_612991",
        "quote": "I have a strong and strange character, and I've rarely met directors who knew what to do with this character. One of the few who did was my father, and in the theatre, Arthur Nauzyciel.",
        "author": {
            "name": "Lou Doillon",
            "url": "/quotes/lou_doillon_612991"
        },
        "topics": [
            {
                "name": "Strong",
                "url": "/topics/strong"
