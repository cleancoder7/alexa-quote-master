            {
                "name": "Arthur",
                "url": "/topics/arthur"
            },
            {
                "name": "Directors",
                "url": "/topics/directors"
            }
        ]
    },
    {
        "url": "/quotes/lou_doillon_612992",
        "quote": "I listen to a variety of music. The only common point is strong lyrics; I'm more obsessed with lyrics than music. I need to hear a form of truth, and if it's a hard truth, even better.",
        "author": {
            "name": "Lou Doillon",
            "url": "/quotes/lou_doillon_612992"
        },
        "topics": [
            {
                "name": "Strong",
                "url": "/topics/strong"
