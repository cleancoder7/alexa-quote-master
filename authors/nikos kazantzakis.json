        ]
    },
    {
        "url": "/quotes/nikos_kazantzakis_746547",
        "quote": "I fight to embrace the entire circle of human activity to the full extent of my ability.",
        "author": {
            "name": "Nikos Kazantzakis",
            "url": "/quotes/nikos_kazantzakis_746547"
