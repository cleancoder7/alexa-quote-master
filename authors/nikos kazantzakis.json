        "author": {
            "name": "Nikos Kazantzakis",
            "url": "/quotes/nikos_kazantzakis_176145"
        },
        "topics": [
            {
                "name": "World",
                "url": "/topics/world"
            },
            {
                "name": "Meaning",
                "url": "/topics/meaning"
            },
            {
                "name": "Hidden",
                "url": "/topics/hidden"
            }
        ]
    },
    {
        "url": "/quotes/nikos_kazantzakis_176146",
