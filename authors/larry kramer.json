            {
                "name": "Been",
                "url": "/topics/been"
            },
            {
                "name": "Note",
                "url": "/topics/note"
            }
        ]
    },
    {
        "url": "/quotes/larry_kramer_522688",
        "quote": "Some reporter called me 'the angriest gay man in the world' or some such. Well, it stuck, but I realized it was very useful.",
        "author": {
            "name": "Larry Kramer",
            "url": "/quotes/larry_kramer_522688"
        },
        "topics": [
            {
                "name": "Reporter",
                "url": "/topics/reporter"
