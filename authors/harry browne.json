            "name": "Harry Browne",
            "url": "/quotes/harry_browne_480777"
        },
        "topics": [
            {
                "name": "American",
                "url": "/topics/american"
            },
            {
                "name": "Bad",
                "url": "/topics/bad"
            },
            {
                "name": "Fingers",
                "url": "/topics/fingers"
            }
