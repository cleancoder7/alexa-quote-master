            {
                "name": "Back",
                "url": "/topics/back"
            },
            {
                "name": "Mostly",
                "url": "/topics/mostly"
            }
        ]
    },
    {
        "url": "/quotes/ann_nocenti_732764",
        "quote": "I'm a huge lover of 'Seven Samurai' and anything Kurosawa ever did. The comedic work out of Japan in terms of martial arts movies, some of them are hilarious.",
        "author": {
            "name": "Ann Nocenti",
            "url": "/quotes/ann_nocenti_732764"
        },
        "topics": [
            {
                "name": "Some",
                "url": "/topics/some"
