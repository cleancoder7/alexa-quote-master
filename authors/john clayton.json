        "topics": [
            {
                "name": "God",
                "url": "/topics/god"
            },
            {
                "name": "Origin",
                "url": "/topics/origin"
            }
        ]
    },
    {
        "url": "/quotes/john_clayton_294682",
        "quote": "Why is it any more reasonable to believe that God has always been than it is to say that matter has always been?",
        "author": {
            "name": "John Clayton",
            "url": "/quotes/john_clayton_294682"
