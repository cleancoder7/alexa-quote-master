            }
        ]
    },
    {
        "url": "/quotes/deeyah_khan_895047",
        "quote": "The self-proclaimed Islamic State cannot tolerate diversity, which is why we must celebrate it.",
        "author": {
            "name": "Deeyah Khan",
            "url": "/quotes/deeyah_khan_895047"
        },
        "topics": [
            {
                "name": "Diversity",
                "url": "/topics/diversity"
            },
            {
                "name": "Which",
                "url": "/topics/which"
            },
