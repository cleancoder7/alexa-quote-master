            {
                "name": "Through",
                "url": "/topics/through"
            },
            {
                "name": "Works",
                "url": "/topics/works"
            }
        ]
    },
    {
        "url": "/quotes/dieter_f_uchtdorf_730669",
        "quote": "None of us is quite as Christlike as we know we should be.",
        "author": {
            "name": "Dieter F. Uchtdorf",
            "url": "/quotes/dieter_f_uchtdorf_730669"
        },
        "topics": [
            {
