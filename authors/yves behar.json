        ]
    },
    {
        "url": "/quotes/yves_behar_757082",
        "quote": "I never felt truly at home in Switzerland.",
        "author": {
            "name": "Yves Behar",
            "url": "/quotes/yves_behar_757082"
        },
        "topics": [
            {
                "name": "Switzerland",
