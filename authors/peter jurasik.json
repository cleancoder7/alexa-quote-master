    {
        "url": "/quotes/peter_jurasik_354829",
        "quote": "I studied with the idea of becoming a Catholic priest.",
        "author": {
            "name": "Peter Jurasik",
            "url": "/quotes/peter_jurasik_354829"
        },
        "topics": [
            {
                "name": "Idea",
