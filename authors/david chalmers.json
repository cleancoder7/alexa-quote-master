        "author": {
            "name": "David Chalmers",
            "url": "/quotes/david_chalmers_251125"
        },
        "topics": [
            {
                "name": "Think",
                "url": "/topics/think"
            },
            {
                "name": "Most",
                "url": "/topics/most"
            },
