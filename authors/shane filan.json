            {
                "name": "Everybody",
                "url": "/topics/everybody"
            },
            {
                "name": "Before",
                "url": "/topics/before"
            }
        ]
    },
    {
        "url": "/quotes/shane_filan_654115",
        "quote": "It's a big deal to bring your family on the road, you know? I'd love to do it sometime, but you have to work out schools and all that.",
        "author": {
            "name": "Shane Filan",
            "url": "/quotes/shane_filan_654115"
        },
        "topics": [
            {
                "name": "Love",
                "url": "/topics/love"
            },
            {
                "name": "Big",
                "url": "/topics/big"
            },
            {
                "name": "Sometime",
                "url": "/topics/sometime"
            }
        ]
    },
