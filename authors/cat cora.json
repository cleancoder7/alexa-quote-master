    {
        "url": "/quotes/cat_cora_442593",
        "quote": "I'm gay, and I was born this way.",
        "author": {
            "name": "Cat Cora",
            "url": "/quotes/cat_cora_442593"
        },
        "topics": [
            {
                "name": "Gay",
                "url": "/topics/gay"
