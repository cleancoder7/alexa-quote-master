[
    {
        "url": "/quotes/juan_felipe_herrera_724339",
        "quote": "I gave my voice to poetry.",
        "author": {
            "name": "Juan Felipe Herrera",
            "url": "/quotes/juan_felipe_herrera_724339"
        },
        "topics": [
