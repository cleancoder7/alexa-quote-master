            {
                "name": "Will",
                "url": "/topics/will"
            },
            {
                "name": "Am",
                "url": "/topics/am"
            }
        ]
    },
    {
        "url": "/quotes/alex_campbell_287750",
        "quote": "I go gladly to my wife and boy, and I leave this world at peace with every one in it and at peace with God.",
        "author": {
            "name": "Alex Campbell",
            "url": "/quotes/alex_campbell_287750"
        },
        "topics": [
            {
                "name": "Wife",
                "url": "/topics/wife"
            },
            {
                "name": "Boy",
