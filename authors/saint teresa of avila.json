    {
        "url": "/quotes/saint_teresa_of_avila_586988",
        "quote": "A soul which gives itself to prayer, either much or little, should on no account be kept within narrow bounds.",
        "author": {
            "name": "Saint Teresa of Avila",
            "url": "/quotes/saint_teresa_of_avila_586988"
        },
