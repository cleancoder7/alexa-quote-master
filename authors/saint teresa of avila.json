        "author": {
            "name": "Saint Teresa of Avila",
            "url": "/quotes/saint_teresa_of_avila_185153"
        },
        "topics": [
            {
                "name": "Nature",
                "url": "/topics/nature"
            },
            {
                "name": "Running",
                "url": "/topics/running"
