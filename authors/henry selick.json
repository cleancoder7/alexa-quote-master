            {
                "name": "Catching",
                "url": "/topics/catching"
            },
            {
                "name": "Films",
                "url": "/topics/films"
            }
        ]
    },
    {
        "url": "/quotes/henry_selick_543915",
        "quote": "There's very few people who want to just make beautiful films that make money, when they can make films that make huge money.",
        "author": {
            "name": "Henry Selick",
            "url": "/quotes/henry_selick_543915"
        },
        "topics": [
            {
                "name": "Want",
                "url": "/topics/want"
