            },
            {
                "name": "Employ",
                "url": "/topics/employ"
            }
        ]
    },
    {
        "url": "/quotes/nick_hanauer_735908",
        "quote": "I have, oddly, two ski houses - trying to sell one.",
        "author": {
            "name": "Nick Hanauer",
            "url": "/quotes/nick_hanauer_735908"
        },
        "topics": [
            {
                "name": "Trying",
                "url": "/topics/trying"
