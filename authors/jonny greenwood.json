    {
        "url": "/quotes/jonny_greenwood_545197",
        "quote": "I'm pampered like you wouldn't believe.",
        "author": {
            "name": "Jonny Greenwood",
            "url": "/quotes/jonny_greenwood_545197"
        },
        "topics": [
