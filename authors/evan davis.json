            {
                "name": "Big",
                "url": "/topics/big"
            },
            {
                "name": "Making",
                "url": "/topics/making"
            }
        ]
    },
    {
        "url": "/quotes/evan_davis_481925",
        "quote": "I actually profoundly think the world's a better place when economics is fairly boring... The more boring the better.",
        "author": {
            "name": "Evan Davis",
            "url": "/quotes/evan_davis_481925"
        },
        "topics": [
            {
                "name": "Think",
