            {
                "name": "Amendment",
                "url": "/topics/amendment"
            },
            {
                "name": "Private",
                "url": "/topics/private"
            }
        ]
    },
    {
        "url": "/quotes/lisa_blatt_751675",
        "quote": "I don't think anyone would describe me as an understated advocate. Several people have told me my argument style is very direct and very blunt, which I find mystifying. How could you ever be anything but direct and blunt?",
        "author": {
            "name": "Lisa Blatt",
            "url": "/quotes/lisa_blatt_751675"
        },
        "topics": [
            {
                "name": "Argument",
                "url": "/topics/argument"
