            {
                "name": "Eager",
                "url": "/topics/eager"
            },
            {
                "name": "Treatment",
                "url": "/topics/treatment"
            }
        ]
    },
    {
        "url": "/quotes/james_mcgreevey_468743",
        "quote": "I'm enjoying prison ministry, particularly with the women in Hudson County Jail who have suffered tremendously in their lives.",
        "author": {
            "name": "James McGreevey",
            "url": "/quotes/james_mcgreevey_468743"
        },
        "topics": [
            {
                "name": "Lives",
                "url": "/topics/lives"
