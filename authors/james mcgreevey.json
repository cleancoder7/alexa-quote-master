            {
                "name": "Kid",
                "url": "/topics/kid"
            },
            {
                "name": "Figure",
                "url": "/topics/figure"
            }
        ]
    },
    {
        "url": "/quotes/james_mcgreevey_444704",
        "quote": "I try to be grateful for the abundance of the blessings that I have, for the journey that I'm on and to relish each day as a gift.",
        "author": {
            "name": "James McGreevey",
            "url": "/quotes/james_mcgreevey_444704"
        },
        "topics": [
            {
                "name": "Gift",
                "url": "/topics/gift"
            },
