            {
                "name": "Go",
                "url": "/topics/go"
            },
            {
                "name": "Read",
                "url": "/topics/read"
            }
        ]
    },
    {
        "url": "/quotes/walter_dean_myers_532711",
        "quote": "We need to tell young people that America was built by men and women of all colors and that the future of this country is dependent on the participation of all of our citizens.",
        "author": {
            "name": "Walter Dean Myers",
            "url": "/quotes/walter_dean_myers_532711"
        },
        "topics": [
