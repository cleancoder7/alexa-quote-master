[
    {
        "url": "/quotes/diana_nyad_445749",
        "quote": "This journey has always been about reaching your own other shore no matter what it is, and that dream continues.",
        "author": {
            "name": "Diana Nyad",
            "url": "/quotes/diana_nyad_445749"
        },
        "topics": [
            {
                "name": "Always",
