            {
                "name": "Moscow",
                "url": "/topics/moscow"
            },
            {
                "name": "Had",
                "url": "/topics/had"
            }
        ]
    },
    {
        "url": "/quotes/david_hallberg_725353",
        "quote": "Having done so many versions, I never felt like an artist in 'Swan Lake.'",
        "author": {
            "name": "David Hallberg",
            "url": "/quotes/david_hallberg_725353"
        },
        "topics": [
