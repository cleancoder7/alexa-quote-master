            {
                "name": "His",
                "url": "/topics/his"
            },
            {
                "name": "Anything",
                "url": "/topics/anything"
            }
        ]
    },
    {
        "url": "/quotes/magnus_scheving_575009",
        "quote": "I realised there were no good role models for kids. Popeye eats spinach, but also smokes and hits people.",
        "author": {
            "name": "Magnus Scheving",
            "url": "/quotes/magnus_scheving_575009"
        },
        "topics": [
            {
                "name": "Models",
