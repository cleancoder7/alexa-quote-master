[
    {
        "url": "/quotes/steven_johnson_527620",
        "quote": "If you look at history, innovation doesn't come just from giving people incentives; it comes from creating environments where their ideas can connect.",
        "author": {
            "name": "Steven Johnson",
            "url": "/quotes/steven_johnson_527620"
        },
        "topics": [
            {
                "name": "Innovation",
                "url": "/topics/innovation"
            },
            {
                "name": "Environments",
                "url": "/topics/environments"
            }
        ]
    },
    {
        "url": "/quotes/steven_johnson_527622",
        "quote": "If we didn't have genetic mutations, we wouldn't have us. You need error to open the door to the adjacent possible.",
        "author": {
            "name": "Steven Johnson",
            "url": "/quotes/steven_johnson_527622"
        },
        "topics": [
            {
                "name": "Door",
                "url": "/topics/door"
