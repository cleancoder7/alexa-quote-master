            {
                "name": "About",
                "url": "/topics/about"
            },
            {
                "name": "I See",
                "url": "/topics/i_see"
            }
        ]
    },
    {
        "url": "/quotes/patti_davis_671708",
        "quote": "I think the earlier stages of Alzheimer's are the hardest. Particularly because the person knows that they are losing awareness. They're aware that they're losing awareness, and you see them struggling.",
        "author": {
            "name": "Patti Davis",
            "url": "/quotes/patti_davis_671708"
        },
        "topics": [
            {
                "name": "Think",
                "url": "/topics/think"
            },
            {
                "name": "Alzheimer",
                "url": "/topics/alzheimer"
            },
            {
                "name": "I Think",
                "url": "/topics/i_think"
            }
        ]
    },
