    {
        "url": "/quotes/patti_davis_671743",
        "quote": "When my father, Ronald Reagan, was running for president in 1980, my mother, Nancy, traveled with him on the campaign trail, but she did not give speeches or even many interviews. She never stood in front of a group of reporters and expounded on her views and opinions.",
        "author": {
            "name": "Patti Davis",
            "url": "/quotes/patti_davis_671743"
        },
        "topics": [
            {
                "name": "Father",
                "url": "/topics/father"
            },
            {
                "name": "Reporters",
                "url": "/topics/reporters"
            },
            {
                "name": "Reagan",
                "url": "/topics/reagan"
            }
        ]
    },
    {
        "url": "/quotes/patti_davis_249752",
        "quote": "I had this odd sibling rivalry with America.",
        "author": {
            "name": "Patti Davis",
            "url": "/quotes/patti_davis_249752"
        },
        "topics": [
            {
                "name": "Sibling",
                "url": "/topics/sibling"
