            },
            {
                "name": "Substances",
                "url": "/topics/substances"
            }
        ]
    },
    {
        "url": "/quotes/henry_spencer_722354",
        "quote": "SpaceX does seem to have had a run of bad luck, with its first three launches all failing.",
        "author": {
            "name": "Henry Spencer",
            "url": "/quotes/henry_spencer_722354"
        },
        "topics": [
            {
                "name": "Luck",
