            {
                "name": "Doing",
                "url": "/topics/doing"
            },
            {
                "name": "Sells",
                "url": "/topics/sells"
            }
        ]
    },
    {
        "url": "/quotes/henry_spencer_722373",
        "quote": "Liquid oxygen is one of the cheapest manufactured substances on Earth. In large quantities, it costs pennies per kilogram - cheaper than milk or beer.",
        "author": {
            "name": "Henry Spencer",
            "url": "/quotes/henry_spencer_722373"
        },
        "topics": [
            {
                "name": "Costs",
