            {
                "name": "Very",
                "url": "/topics/very"
            },
            {
                "name": "Foods",
                "url": "/topics/foods"
            }
        ]
    },
    {
        "url": "/quotes/ted_allen_443540",
        "quote": "Not to sound too much like Christopher Guest in 'Waiting for Guffman,' but on Thanksgiving you're putting on a show!",
        "author": {
            "name": "Ted Allen",
            "url": "/quotes/ted_allen_443540"
