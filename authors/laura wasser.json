            {
                "name": "Counseling",
                "url": "/topics/counseling"
            },
            {
                "name": "However",
                "url": "/topics/however"
            }
        ]
    },
    {
        "url": "/quotes/laura_wasser_619924",
        "quote": "Evening bags should be just big enough for my phone, lipstick, house key, and credit card.",
        "author": {
            "name": "Laura Wasser",
            "url": "/quotes/laura_wasser_619924"
        },
        "topics": [
            {
