[
    {
        "url": "/quotes/asa_gray_243679",
        "quote": "Your candor is worth everything to your cause. It is refreshing to find a person with a new theory who frankly confesses that he finds difficulties, insurmountable, at least for the present.",
        "author": {
            "name": "Asa Gray",
            "url": "/quotes/asa_gray_243679"
        },
        "topics": [
            {
                "name": "New",
                "url": "/topics/new"
            },
