            },
            {
                "name": "Became",
                "url": "/topics/became"
            }
        ]
    },
    {
        "url": "/quotes/jack_dangermond_731713",
        "quote": "I prefer to find craftspeople I can be colleagues with and who take an area of responsibility and run with it.",
        "author": {
            "name": "Jack Dangermond",
            "url": "/quotes/jack_dangermond_731713"
        },
