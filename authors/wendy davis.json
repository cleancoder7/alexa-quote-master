            {
                "name": "Other",
                "url": "/topics/other"
            },
            {
                "name": "Rather",
                "url": "/topics/rather"
            }
        ]
    },
    {
        "url": "/quotes/wendy_davis_631926",
        "quote": "Real Texans believe in looking out for each other. We believe in honoring our mothers and fathers and keeping our smallest residents - our children - healthy.",
        "author": {
            "name": "Wendy Davis",
            "url": "/quotes/wendy_davis_631926"
        },
        "topics": [
            {
                "name": "Other",
                "url": "/topics/other"
