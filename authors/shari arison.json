            }
        ]
    },
    {
        "url": "/quotes/shari_arison_645302",
        "quote": "A yacht is not for me. I don't enjoy it.",
        "author": {
            "name": "Shari Arison",
            "url": "/quotes/shari_arison_645302"
        },
