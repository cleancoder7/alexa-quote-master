            {
                "name": "Direct",
                "url": "/topics/direct"
            }
        ]
    },
    {
        "url": "/quotes/shari_arison_645264",
        "quote": "I spent my entire life being afraid. I was afraid of everything - height, flights - I had no idea why.",
        "author": {
            "name": "Shari Arison",
            "url": "/quotes/shari_arison_645264"
        },
