            "name": "Anat Cohen",
            "url": "/quotes/anat_cohen_783983"
        },
        "topics": [
            {
                "name": "Jazz",
                "url": "/topics/jazz"
            },
            {
                "name": "Reason",
                "url": "/topics/reason"
