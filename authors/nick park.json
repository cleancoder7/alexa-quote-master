            {
                "name": "Image",
                "url": "/topics/image"
            },
            {
                "name": "Rather",
                "url": "/topics/rather"
            }
        ]
    },
    {
        "url": "/quotes/nick_park_406562",
        "quote": "We have to look forward and keep filming new films and not get stuck in the past.",
