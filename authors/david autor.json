            },
            {
                "name": "Head",
                "url": "/topics/head"
            },
            {
                "name": "Professor",
                "url": "/topics/professor"
            }
        ]
    },
    {
        "url": "/quotes/david_autor_875314",
        "quote": "There's never been a better time to be a worker with special skills or the right education, because these people can use technology to create and capture value.",
        "author": {
            "name": "David Autor",
            "url": "/quotes/david_autor_875314"
        },
        "topics": [
            {
                "name": "Education",
                "url": "/topics/education"
