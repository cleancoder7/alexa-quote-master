[
    {
        "url": "/quotes/ellen_stofan_728922",
        "quote": "There's a huge question of whether you really need water for life.",
        "author": {
            "name": "Ellen Stofan",
            "url": "/quotes/ellen_stofan_728922"
        },
        "topics": [
            {
