            {
                "name": "Anxieties",
                "url": "/topics/anxieties"
            },
            {
                "name": "How",
                "url": "/topics/how"
            }
        ]
    },
    {
        "url": "/quotes/duane_michals_106039",
        "quote": "Trust that little voice in your head that says 'Wouldn't it be interesting if...'; And then do it.",
        "author": {
            "name": "Duane Michals",
            "url": "/quotes/duane_michals_106039"
        },
        "topics": [
            {
