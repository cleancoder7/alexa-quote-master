            {
                "name": "Important",
                "url": "/topics/important"
            },
            {
                "name": "See",
                "url": "/topics/see"
            }
        ]
    },
    {
        "url": "/quotes/duane_michals_605806",
        "quote": "You can't see fear or lust; you can't photograph someone's anxieties, how disappointment feels. Photographs are approximations.",
        "author": {
            "name": "Duane Michals",
            "url": "/quotes/duane_michals_605806"
        },
        "topics": [
            {
                "name": "Feels",
                "url": "/topics/feels"
