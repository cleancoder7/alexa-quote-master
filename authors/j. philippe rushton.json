    {
        "url": "/quotes/j_philippe_rushton_296754",
        "quote": "Deconstructing the concept of race not only conflicts with people's tendency to classify and build family histories according to common descent but also ignores the work of biologists studying non-human species.",
        "author": {
            "name": "J. Philippe Rushton",
            "url": "/quotes/j_philippe_rushton_296754"
        },
        "topics": [
            {
                "name": "Studying",
                "url": "/topics/studying"
            },
            {
                "name": "Race",
                "url": "/topics/race"
            },
