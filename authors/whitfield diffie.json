            }
        ]
    },
    {
        "url": "/quotes/whitfield_diffie_675945",
        "quote": "No right of private conversation was enumerated in the Constitution. I suppose it never occurred to anyone at the time that it could be prevented.",
        "author": {
            "name": "Whitfield Diffie",
            "url": "/quotes/whitfield_diffie_675945"
        },
        "topics": [
            {
                "name": "Constitution",
                "url": "/topics/constitution"
