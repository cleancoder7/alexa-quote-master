            {
                "name": "Young",
                "url": "/topics/young"
            },
            {
                "name": "Before",
                "url": "/topics/before"
            }
        ]
    },
    {
        "url": "/quotes/knut_hamsun_316897",
        "quote": "Today riches and honours have been lavished on me, but one gift has been lacking, the most important one of all, the only one that matters, the gift of youth.",
        "author": {
            "name": "Knut Hamsun",
            "url": "/quotes/knut_hamsun_316897"
        },
        "topics": [
