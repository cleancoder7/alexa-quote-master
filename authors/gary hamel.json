        "topics": [
            {
                "name": "Risk-Taking",
                "url": "/topics/risk-taking"
            },
            {
                "name": "Regulatory",
                "url": "/topics/regulatory"
            }
        ]
    },
    {
        "url": "/quotes/gary_hamel_528643",
        "quote": "I am an ardent supporter of capitalism - but I also understand that while individuals have inalienable, God-given rights, corporations do not.",
        "author": {
            "name": "Gary Hamel",
            "url": "/quotes/gary_hamel_528643"
