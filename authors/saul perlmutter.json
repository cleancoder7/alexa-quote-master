    {
        "url": "/quotes/saul_perlmutter_417816",
        "quote": "This is the kind of discovery that resonates.",
        "author": {
            "name": "Saul Perlmutter",
            "url": "/quotes/saul_perlmutter_417816"
        },
        "topics": [
            {
                "name": "Discovery",
