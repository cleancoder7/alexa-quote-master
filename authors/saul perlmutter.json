[
    {
        "url": "/quotes/saul_perlmutter_417830",
        "quote": "It is a tough choice between ending up in the cold or ending up in a fiery blast.",
        "author": {
            "name": "Saul Perlmutter",
            "url": "/quotes/saul_perlmutter_417830"
        },
        "topics": [
            {
