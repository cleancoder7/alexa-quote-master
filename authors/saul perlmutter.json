            }
        ]
    },
    {
        "url": "/quotes/saul_perlmutter_417825",
        "quote": "If you're puzzled by what dark energy is, you're in good company.",
        "author": {
            "name": "Saul Perlmutter",
            "url": "/quotes/saul_perlmutter_417825"
        },
        "topics": [
            {
                "name": "Dark",
