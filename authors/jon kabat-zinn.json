    {
        "url": "/quotes/jon_kabatzinn_637556",
        "quote": "The real meditation practice is how we live our lives from moment to moment to moment.",
        "author": {
            "name": "Jon Kabat-Zinn",
            "url": "/quotes/jon_kabatzinn_637556"
