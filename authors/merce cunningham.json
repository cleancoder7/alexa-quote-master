            {
                "name": "Fleeting",
                "url": "/topics/fleeting"
            },
            {
                "name": "Image",
                "url": "/topics/image"
            }
        ]
    },
    {
        "url": "/quotes/merce_cunningham_802790",
        "quote": "You can get fixed ideas, and it can get restrictive. So, I try to put myself in a precarious position.",
        "author": {
            "name": "Merce Cunningham",
            "url": "/quotes/merce_cunningham_802790"
        },
        "topics": [
